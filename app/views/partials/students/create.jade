h1 Register Student - Enter {{current_step.name}}

div.row
	div.col-md-3
		ul.nav.nav-pills.nav-stacked
			li(ng-repeat="step in wizard_steps", ng-class="{'active':current_step === step, 'disabled':step.enabled != true}")
				a(href='{{step.id}}') {{step.name}}

	div.col-md-9
		div.wizard_body.panel(ui-view="create-student-wzd-body")