h1 {{currentProgram.name}}
br

h2 Classes
.row
	.col-md-12
		div.gridStyle(ng-grid='classGridOptions')


h2 Ranks
table.table
	tr
		th.col-md-4.col-sm-4.col-lg-4.col-xs-4 Name
		th.col-md-4.col-sm-4.col-lg-4.col-xs-4 Order 
		th.col-md-4.col-sm-4.col-lg-4.col-xs-4 Sub-ranks
		th Actions
	tr(ng-repeat='rank in currentProgram.rankObjs')
		td 
			{{rank.name}}
		td
			{{rank.rankOrder}}
		td
			| SUB-RANKS
		td
			button.btn.btn-info.glyphicon.glyphicon-eye-open.pull-right(ng-click='goToViewRank(rank)')

input.btn.btn-warning.pull-right(type='reset', value='Back', ng-click='backToHome()')

div(class='clearfix')